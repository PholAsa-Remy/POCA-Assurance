{
  "name": "nest-typescript-starter",
  "private": true,
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "containers:db:start": "docker-compose up -d db",
    "containers:stop": "docker-compose down",
    "containers:db:erase": "docker volume rm poca_db-data",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "dryrun": "./node_modules/typeorm/cli.js migration:generate --dr  . ",
    "dryrun2": "./node_modules/typeorm/cli.js migration:generate --dr  --dataSource ./src/shared/typeorm/typeorm.service.ts  . "
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/typeorm": "^9.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "ejs": "^3.1.8",
    "express-session": "^1.17.3",
    "hbs": "^4.2.0",
    "jspdf": "^2.5.1",
    "nodemailer": "^6.8.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5",
    "typeorm": "^0.3.9"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.5",
    "@types/jest": "^28.1.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.7.13",
    "@types/nodemailer": "^6.4.6",
    "@types/passport-jwt": "^3.0.7",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.2",
    "prettier": "^2.7.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.5",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.8.2",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/app.module.ts",
      "<rootDir>/main.ts",
      "<rootDir>/shared/valueobjects/ValueObject.ts",
      "<rootDir>/auth/auth.module.ts",
      "<rootDir>/migrations/MigrationCustomer.ts",
      "<rootDir>/migrations/MigrationQuote.ts",
      "<rootDir>/auth/jwt/jwt.constants.ts",
      "<rootDir>/auth/jwt/jwt-auth.guard.ts",
      "<rootDir>/auth/command/auth.command.ts",
      "<rootDir>/auth/local/local-auth.guard.ts",
      "<rootDir>/base/base.module.ts",
      "<rootDir>/base/aboutus/aboutus.module.ts",
      "<rootDir>/base/contact/contact.module.ts",
      "<rootDir>/base/customer/customer.module.ts",
      "<rootDir>/base/payment/payment.module.ts",
      "<rootDir>/base/reportsinister/reportsinister.module.ts",
      "<rootDir>/base/quote/quote.module.ts",
      "<rootDir>/base/userhome/userhome.module.ts",
      "<rootDir>/common/helper/env.helper.ts",
      "<rootDir>/shared/typeorm/typeorm.service.ts",
      "<rootDir>/auth/local/local.strategy.ts",
      "<rootDir>/auth/jwt/jwt.strategy.ts",
      "<rootDir>/base/pdf/pdf.module.ts",
      "<rootDir>/shared/decorators/parse.boolean.ts"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
